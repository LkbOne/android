<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <android.support.design.widget.AppBarLayout
        android:layout_width="match_parent"
        android:layout_height="25dp"
        android:theme="@style/AppTheme.AppBarOverlay"/>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <!--<ImageView-->
            <!--android:id="@+id/imageView7"-->
            <!--android:layout_width="56dp"-->
            <!--android:layout_height="match_parent"-->
            <!--app:srcCompat="@android:drawable/star_big_on"-->
            <!--tools:srcCompat="@tools:sample/avatars[0]" />-->

        <android.support.design.widget.TextInputEditText
            android:id="@+id/UserNameTIET"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:hint="用户名"
            android:inputType="textPersonName" />

    </TableRow>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <!--<ImageView-->
            <!--android:id="@+id/imageView8"-->
            <!--android:layout_width="wrap_content"-->
            <!--android:layout_height="wrap_content"-->
            <!--android:visibility="visible"-->
            <!--app:srcCompat="@android:drawable/star_big_on"-->
            <!--tools:srcCompat="@android:drawable/sym_action_call" />-->

        <android.support.design.widget.TextInputEditText
            android:id="@+id/PhoneTIET"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="电话"
            android:inputType="phone" />

    </TableRow>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <!--<ImageView-->
            <!--android:id="@+id/imageView2"-->
            <!--android:layout_width="54dp"-->
            <!--android:layout_height="wrap_content"-->
            <!--app:srcCompat="@android:drawable/star_big_on" />-->

        <android.support.design.widget.TextInputEditText
            android:id="@+id/AccountTIET"
            android:layout_width="341dp"
            android:layout_height="wrap_content"
            android:hint="账户"
            android:inputType="text" />

    </TableRow>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <!--<ImageView-->
            <!--android:id="@+id/imageView3"-->
            <!--android:layout_width="wrap_content"-->
            <!--android:layout_height="wrap_content"-->
            <!--app:srcCompat="@android:drawable/star_big_on" />-->

        <android.support.design.widget.TextInputEditText
            android:id="@+id/PasswordTIET"
            android:layout_width="234dp"
            android:layout_height="wrap_content"
            android:hint="密码"
            android:inputType="textPassword"
            android:textAlignment="viewStart" />

    </TableRow>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <!--<ImageView-->
            <!--android:id="@+id/imageView4"-->
            <!--android:layout_width="wrap_content"-->
            <!--android:layout_height="wrap_content"-->
            <!--app:srcCompat="@android:drawable/star_big_on" />-->

        <android.support.design.widget.TextInputEditText
            android:id="@+id/ComfirmTIET"
            android:layout_width="341dp"
            android:layout_height="wrap_content"
            android:hint="密码确认"
            android:inputType="textPassword" />

    </TableRow>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <!--<ImageView-->
            <!--android:id="@+id/imageView5"-->
            <!--android:layout_width="wrap_content"-->
            <!--android:layout_height="wrap_content"-->
            <!--app:srcCompat="@android:drawable/star_big_on" />-->

        <android.support.design.widget.TextInputEditText
            android:id="@+id/EmailTIET"
            android:layout_width="341dp"
            android:layout_height="wrap_content"
            android:hint="邮箱"
            android:inputType="textWebEmailAddress" />

    </TableRow>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <!--<ImageView-->
            <!--android:id="@+id/imageView6"-->
            <!--android:layout_width="wrap_content"-->
            <!--android:layout_height="wrap_content"-->
            <!--app:srcCompat="@android:drawable/star_big_on" />-->

        <android.support.design.widget.TextInputEditText
            android:id="@+id/DescTIET"
            android:layout_width="341dp"
            android:layout_height="wrap_content"
            android:hint="个人宣言"
            android:inputType="text" />

    </TableRow>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center">

        <Button
            android:id="@+id/RegisterBtn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="提交"
            android:textAlignment="center" />

    </TableRow>
</TableLayout>